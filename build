#!/bin/bash -e
cd $(dirname $0)

PATH=$HOME/go/bin:$PATH
unset GOPATH

go mod download

./builtintest/clean

./copy-threadsafe-builtins

./run-statics-tool

echo
echo Installing...
go install .

type runtemplate

echo
echo Generating tests...
go generate -v ./builtintest/...

echo
echo Testing...
go test ./...
#go test $coveralls_testflags $GOBUILDFLAG $@ .
